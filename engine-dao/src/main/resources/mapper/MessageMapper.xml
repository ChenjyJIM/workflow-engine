<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.graduate.engine.mapper.MessageMapper">

    <!-- 根据ID获取信息 -->
    <select id="queryByMessageId" resultType="com.graduate.engine.model.Message">
        select *
        from message
        where message_id = #{messageId}
    </select>
</mapper>
