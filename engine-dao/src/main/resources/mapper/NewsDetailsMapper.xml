<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.graduate.engine.mapper.NewsDetailsMapper">
    <select id="likeSelectByTitle" resultType="long">
        select details_id from news_details where news_title like concat('%',#{title},'%')
    </select>
</mapper>
