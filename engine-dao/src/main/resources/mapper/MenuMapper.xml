<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.graduate.engine.mapper.MenuMapper">
    <select id="queryListByParentId" resultType="com.graduate.engine.model.Menu">
        select * from menu where parent_id = #{parentId} order by order_num asc
    </select>
</mapper>
